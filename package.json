{
  "name": "physical-svg",
  "version": "0.0.1",
  "description": "Library to animate svg image path based on 2d physics",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es)"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardk123/physical-svg.git"
  },
  "keywords": [
    "svg",
    "physics",
    "font"
  ],
  "author": "Richard Kolisek richard.kolisek@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/richardk123/physical-svg/issues"
  },
  "homepage": "https://github.com/richardk123/physical-svg#readme",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "bezier-js": "^6.1.0",
    "matter-js": "^0.18.0",
    "svg-path-parser": "^1.1.0",
    "@types/bezier-js": "^4.1.0",
    "@types/matter-js": "^0.18.2",
    "@types/uuid": "^8.3.4",
    "@types/svg-path-parser": "^1.1.3",
    "uuid": "^9.0.0"
  }
}
